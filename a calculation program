#include <stdio.h>
#include <math.h>

int main() {
    int x;
  int x2, x4, x6, x8;
  int calculate;
  printf("enter x: ");
  scanf("%d", &x);
   

  x2= x*x;
  x4= x*x*x*x;
  x6= x*x*x*x*x*x;
  x8= x*x*x*x*x*x*x*x;
  
  
  
  printf("result for x2 is %d ,x4= %d,x6= %d and x8= %d", x2, x4, x6, x8 );
  
  
   
    return 0;
}
