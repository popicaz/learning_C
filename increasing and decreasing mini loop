#include <stdio.h>

int main() {
  
  int i ,j,k ,l;
 int h;
 printf("enter h: ");
 scanf("%d", &h);
 for(i = 1; i <= h; i++) {
     for(j =1; j <= i; j++)
     printf("%d", j);
     printf("\n");
 }
 for(k = h - 1; k >= 1; k--) {
     for(l = 1; l <= k; l++)
     printf("%d", l);
     printf("\n");
 }

    return 0;
}
