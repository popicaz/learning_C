#include <stdio.h>
#include <stdlib.h>

int main() {
    
    int x, y;
  printf("enter num1: ");
  scanf("%d", &x);
   printf("enter num2: ");
  scanf("%d", &y);
    
 if(x > 0 && y > 0)
  printf("1st quadrant");
  if (x < 0 && y > 0)
  printf("2nd quadrant");
  if(x < 0 && y < 0)
  printf("3rd quadrant");
  if(x > 0 && y < 0)
  printf("4rd quadrant");
  
    return 0;
}
